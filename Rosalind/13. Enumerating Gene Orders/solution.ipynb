   "source": [
    "n = 5\n",
    "\n",
    "n_list = []\n",
    "\n",
    "while n > 0:\n",
    "    n_list.append(n)\n",
    "    n -= 1\n",
    "\n",
    "print(n_list)\n",
    "from itertools import permutations\n",
    "\n",
    "\n",
    "permute = [tuple(map(str, tup)) for tup in list(permutations(n_list,5))]\n",
    "\n",
    "print(len(permute))\n",
    "\n",
    "for i in permute:\n",
    "    print(' '.join(i))\n"
   ]
